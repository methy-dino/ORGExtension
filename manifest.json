{
  "manifest_version": 3,
  "name": "OrgExtension",
  "description": "Agenda auxiliar integrada ao navegador.",
  "version": "0.0.1",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["notifications", "storage", "alarms"]
}

