{
  "manifest_version": 3,
  "name": "OrgExtension",
  "description": "Agenda auxiliar integrada ao navegador",
  "version": "0.0.1",
  "icons": {
    "16": "icones/16x16.png",
    "32": "icones/32x32.png",
    "48": "icones/48x48.png",
    "128": "icones/128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
		"default_popup":"popup.html",
    "default_icon": {
			"16": "icones/16x16.png",
			"32": "icones/32x32.png",
			"48": "icones/48x48.png",
			"128": "icones/128x128.png"
    }
  },
  "permissions": ["scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
