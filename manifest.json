{
  "manifest_version": 2,
  "name": "OrgExtension",
  "description": "Agenda auxiliar integrada ao navegador.",
  "version": "0.0.1",
		"icons": {
    "16": "icones/16x16.png",
    "32": "icones/32x32.png",
    "64": "icones/64x64.png",
    "128": "icones/128x128.png"
		},
  "background": {
    "scripts": ["background.js"],
    "type": "module",
		"persistent": true
  },
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "My Extension Popup"
	},
  "permissions": ["notifications", "storage", "alarms"]
}
